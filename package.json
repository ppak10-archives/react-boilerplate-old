{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "Front-end Boilerplate for React Project",
  "main": "index.js",
  "scripts": {
    "dev": "webpack-dev-server --env=development",
    "build": "webpack --env=production",
    "start": "npm run build; node src/server"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write --print-width 80",
      "eslint --fix",
      "eslint",
      "git add"
    ],
    "*.jsx": [
      "prettier --write --print-width 80",
      "eslint --fix",
      "eslint",
      "git add"
    ],
    "*.scss": [
      "stylelint --fix",
      "stylelint --syntax=scss",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ppak10/react-boilerplate.git"
  },
  "author": "ppak10",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ppak10/react-boilerplate/issues"
  },
  "homepage": "https://github.com/ppak10/react-boilerplate#readme",
  "dependencies": {
    "@babel/core": "^7.3.4",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.5.0",
    "babel-loader": "^8.0.5",
    "bootstrap": "^4.3.1",
    "css-loader": "^2.1.1",
    "express": "^4.16.4",
    "jquery": "^3.3.1",
    "node-sass": "^4.11.0",
    "popper.js": "^1.14.7",
    "postcss-loader": "^3.0.0",
    "precss": "^4.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-router-dom": "^5.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "copy-webpack-plugin": "^5.0.0",
    "eslint": "^5.15.1",
    "eslint-config-google": "^0.12.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.5.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "prettier": "^1.16.4",
    "stylelint": "^9.10.1",
    "stylelint-config-sass-guidelines": "^5.3.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-order": "^2.1.0",
    "stylelint-scss": "^3.5.4",
    "webpack-dev-server": "^3.2.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
